{
  "name": "ambry",
  "license": "MIT",
  "main": "./entry.js",
  "version": "1.0.0",
  "scripts": {
    "start": "APP_VARIANT=development expo start",
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios",
    "prebuild": "APP_VARIANT=development expo prebuild",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "doctor": "bunx expo-doctor",
    "expo-upgrade": "bun install expo && bun expo install --fix",
    "android-preview": "bunx eas build --platform android --profile preview --local",
    "ios-preview": "bunx eas build --platform ios --profile preview --local",
    "codegen": "bunx graphql-codegen --config codegen.ts",
    "codegen-watch": "bunx graphql-codegen --config codegen.ts --watch",
    "generate-migrations": "bunx drizzle-kit generate"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-track-player",
          "@react-hook/previous",
          "drizzle-orm",
          "expo-drizzle-studio-plugin",
          "tailwindcss",
          "use-debounce"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-hook/previous": "^1.0.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "drizzle-orm": "^0.36.3",
    "expo": "~52.0.9",
    "expo-blur": "~14.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.3",
    "expo-drizzle-studio-plugin": "^0.1.0",
    "expo-file-system": "~18.0.4",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-image": "~2.0.1",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.7",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.12",
    "expo-sqlite": "~15.0.3",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.3",
    "expo-web-browser": "~14.0.1",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-edge-to-edge": "^1.1.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keyboard-controller": "^1.14.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-svg": "15.8.0",
    "react-native-track-player": "^4.1.1",
    "react-native-web": "~0.19.13",
    "tailwindcss": "^3.4.14",
    "use-debounce": "^10.0.4",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@babel/core": "^7.26.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@parcel/watcher": "^2.5.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-inline-import": "^3.0.0",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.6.3"
  },
  "private": true,
  "patchedDependencies": {
    "react-native-screens@4.1.0": "patches/react-native-screens@4.1.0.patch",
    "react-native-track-player@4.1.1": "patches/react-native-track-player@4.1.1.patch"
  }
}
